<h2>Editing notebook</h2>
  <%= simple_form_for @notebook do |f| %>

      <%= f.input :asin, hint:'asin da amazon que garante que nÃ£o tenha notebook repetido' %>
      <%= f.input :bar_code %>
      <%= f.input :full_price %>
      <%= f.input :offer_price %>
      <%= f.input :brand %>
      <%= f.input :modelo %>
      <%= f.input :processor %>
      <%= f.input :color %>
      <%= f.input :screen %>
      <%= f.input :ram %>
      <%= f.input :hd %>
      <%= f.input :ssd %>
      <%= f.input :placa_video %>
      <%= f.input :amazon_sales_rank %>
      <%= f.input :guarantee %>
      <%= f.input :link_amazon, hint:'Coloque o link da amazon sendo seupc como afiliado para ganharmos porcentagem na compra' %>


      <%= f.input :photo, as: :file %>
      <%= f.submit class: 'btn btn-primary' %>

  <% end %>

